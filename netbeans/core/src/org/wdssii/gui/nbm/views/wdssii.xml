<?xml version="1.0" encoding="UTF-8"?>
<?wdssii version="2.0"?>
<setup>
<perspectives show="Basic">
<perspective class="Basic" loadOnStartup="yes"/>
<perspective class="VSlice" loadOnStartup="yes"/>
</perspectives>

<charts default="VSlice">
<chart name="DataRangeValue">
  <attr name="show" type="boolean" val="yes"/>
  <attr name="guiString" type="string" val="Range Data Table"/>
</chart>
<chart name="TimeTrend">
  <attr name="show" type="boolean" val="yes"/>
  <attr name="guiString" type="string" val="Time Trend Chart of current product"/>
</chart>
<chart name="VSlice">
  <attr name="show" type="boolean" val="yes"/>
  <attr name="guiString" type="string" val="Vertical Slice"/>
</chart>
<chart name="MemoryUsage">
  <attr name="show" type="boolean" val="yes"/>
  <attr name="guiString" type="string" val="Memory Usage"/>
</chart>
</charts>

</setup>